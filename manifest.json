{
    "manifest_version": 2,

    "name": "Animate Generator",
    "version": "1.1",
    "description": "Extends the Developer Tools, adding an audit category that finds broken links on the inspected page.",
    "background": {
        "page": "devtools/background.html"
        //"persistent": false
    },
    "devtools_page": "devtools/devtools.html",
    "permissions": [
        "experimental",
        "tabs",
        "http://*/*",
            "https://*/*"
    ],
    "sandbox": {
        "pages": ["devtools/sandbox.html"]
    }
}
